  "dependencies": {
    "body-parser": "1.19.1",
    "concurrently": "5.3.0",
    "express": "4.17.1",
    "express-handlebars": "4.0.6",
    "express-session": "1.17.1",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.7.3",
    "nodemon": "2.0.7",
    "path": "0.12.7",
    "request": "2.88.2",
    "socket.io": "2.4.1"
  }
}
