{
  "name": "react_nodejs_project",
  "version": "1.0.0",
  "description": "react and nodejs project",
  "main": "index.js",
  "engines": {
    "node": "10.15.3"
  },
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon server/index.js --ignore client",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start-build": "node server/index.js",
    "build": "npm install && cd client && npm install && npm run build"
  },
  "keywords": [
    "react",
    "app",
    "nodejs",
    "reducer",
    "redux",
    "saga",
    "middleware"
  ],
  "author": "oana popescu",
  "license": "ISC",
  "repository": { 
    "type": "git", 
    "url": "https://github.com/oanapopescu93/website" 
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.21",
    "path": "^0.12.7",
    "socket.io": "2.4.1"
  }
}
